{
    "name": "php-lint-this-field",
    "displayName": "php-lint$this->$field",
    "description": "Lint for php code ,show warning when you write '$this->$'",
    "version": "0.0.2",
    "publisher": "ichynul",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Linters"
    ],
    "keywords": [
        "php lint",
        "php $this",
        "php $this->$"
    ],
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "php_this",
            "properties": {
                "php_this.warnningLevel": {
                    "type": "number",
                    "default": 0,
                    "description": "warnning level(0:Error|1:Warning|2:Information|3:Hint), default is 0."
                }
            }
        }
    },
    "repository":"https://github.com/ichynul/php-this-fix.git",
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}